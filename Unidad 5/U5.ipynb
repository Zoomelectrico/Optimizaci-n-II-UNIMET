    "memo = {}\n",
    "\n",
    "def factorial(n):\n",
    "    \n",
    "    if (n == 1):\n",
    "        return 1\n",
    "    else:\n",
    "        memo[n] = n * factorial(n - 1)\n",
    "        return memo.get(n)\n",
    "\n",
    "print(factorial(10)) # 120"
   ]
